            </validationRule>
        </fields>
        <fields>
            <name>housing_RightsToDenyRequest</name>
            <dataType>Boolean</dataType>
            <fieldText>RIO Education reserves the right to deny a request of residency without explanation.</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>true</isRequired>
            <validationRule>
